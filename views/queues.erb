<!DOCTYPE html>
<html>

  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <title>Cluster queues</title>
    <link rel="stylesheet" type="text/css" href="/reset.css" />
    <link rel="stylesheet" type="text/css" href="/cluster.css" />
    <link rel="stylesheet" type="text/css" href="/user-colors.css" />
  </head>

  <body class="all-owners">

    <div id="header">
      <p><span>Queues</span><span id="timestamp">Loading...</span></p>
      <p class="nav"><a href="/chart">History</a><a href="/memory">Memory</a><a href="/matlab">Matlab</a><a href="/resources">Resources</a></p>
      <ul id="owner-toggle"></ul>
    </div>

    <table cellspacing="0" class="queues">
      <tbody>
        <tr id="detailed-job-list">
          <td rowspan="4">
            <div id="job-table-popup">
              <h2 class="owner-1" id="job-details-popup-header">gvantulder &ndash; 13 running in week</h2>
              <a class="close-popup" href="#"><i class="icon-remove-sign"></i></a>
              <table cellspacing="0" class="job-details" id="job-details-popup-table"></table>
            </div>
          </td>
          <td colspan="4"></td>
        </tr>
        <tr id="queued-jobs" class="queued-jobs"></tr>
        <tr id="running-jobs" class="running-jobs"></tr>
        <tr id="finished-jobs" class="finished-jobs"></tr>
      </tbody>
    </table>

    <script src="//ajax.googleapis.com/ajax/libs/mootools/1.4.5/mootools-yui-compressed.js"></script>
    <script type="text/javascript" src="/queues.js"></script>

  </body>

</html>

